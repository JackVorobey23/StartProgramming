        <Style x:Key="MenuButton" TargetType="RadioButton">
            <Setter Property="Foreground" Value="#FFC2C2C2"></Setter>
            <Setter Property="Background" Value="#00FFFFFF"></Setter>
            <Setter Property="Padding" Value="0,0,0,0"></Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter Property="Foreground" Value="#C9D5FF"/>
                    <Setter Property="Background" Value="#37284E"/>
                </Trigger>
                <Trigger Property="IsChecked" Value="True">
                    <Setter Property="Foreground" Value="#FFE977"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="MenuRectangle" TargetType="Rectangle">
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter Property="StrokeThickness" Value="30"/>
                    <Setter Property="Stroke" Value="#66F17777"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="Genre_Button" TargetType="Border">
